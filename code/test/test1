int s = socket(AF_INET,SOCK_STREAM,0);
Bind(s,...);
listen(s,...);

int epfd = epoll_create(...);
epoll_ctl(epfd,...);//将所有需要监听的socket添加到epfd中，向树上添加需要坚挺的fd。


while(1){
    int = epoll_wait(...);//监听
    
